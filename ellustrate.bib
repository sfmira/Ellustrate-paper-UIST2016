	year = {2014},
	keywords = {creativity, novices, painting},
	pages = {419--428},
	file = {ACM Full Text PDF:/Users/cearto/Library/Application Support/Zotero/Profiles/ah6xzwmm.default/zotero/storage/2I36IKXI/Benedetti et al. - 2014 - Painting with Bob Assisted Creativity for Novices.pdf:application/pdf}
