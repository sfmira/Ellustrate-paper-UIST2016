@inproceedings{baafi_toolkit_2011,
	address = {New York, NY, USA},
	series = {{TEI} '11},
	title = {A {Toolkit} for {Tinkering} with {Tangibles} \& {Connecting} {Communities}},
	isbn = {978-1-4503-0478-8},
